@{
}
@model IEnumerable<string>

<div class="btn-group-vertical btn-block">
    <a asp-controller="Home" asp-action="Index" asp-route-bookType="" asp-route-pageNum="1" class="btn btn-outline-primary">Home</a>
    <br />
    @foreach (string t in Model)
    {

        <a asp-controller="Home" asp-action="Index" asp-route-bookType="@t" asp-route-pageNum="1"
           class="btn @(ViewBag.SelectedType == t ? "btn-primary" : "btn-outline-secondary") btn-block">@t</a>
        <br />
    }
</div>